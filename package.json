{
  "name": "smartcar-sdk",
  "version": "1.1.2",
  "description": "nodejs sdk for the smartcar platform",
  "main": "index.js",
  "dependencies": {
    "bluebird": "^3.4.1",
    "lodash": "^4.13.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "dirty-chai": "^1.2.2",
    "mocha": "^3.2.0",
    "nock": "^8.0.0",
    "nyc": "^10.0.0"
  },
  "scripts": {
    "test": "npm run test-unit",
    "test-unit": "cross-env NODE_ENV=development mocha --ui=tdd --recursive --timeout=10000",
    "test-integration": "cross-env NOCK_OFF=true npm run test-unit",
    "coveralls": "npm run cover-unit && $(npm bin)/coveralls < coverage/lcov.info",
    "cover-unit": "nyc npm test -s",
    "cover-integration": "NOCK_OFF=true npm run cover-unit",
    "jsdoc": "jsdoc -c .jsdocrc ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartcar/node-sdk.git"
  },
  "author": "Zane Bradley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartcar/node-sdk/issues"
  },
  "homepage": "https://github.com/smartcar/node-sdk#readme"
}
